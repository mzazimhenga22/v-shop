{
  "name": "myapp",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm:dev:backend\" \"npm:dev:frontend:wait\"",
    "dev:backend": "nodemon --watch backend --ext ts --exec \"ts-node backend/src/index.ts\"",
    "dev:frontend": "vite",
    "dev:frontend:wait": "wait-on http://localhost:4000/ping && vite",
    "build": "npm run build:backend && npm run build:frontend",
    "build:frontend": "vite build",
    "build:backend": "tsc -p backend/tsconfig.json",
    "lint": "eslint .",
    "preview": "vite preview",
    "start": "node backend/dist/index.js"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.4",
    "@lottiefiles/react-lottie-player": "^3.6.0",
    "@react-google-maps/api": "^2.20.7",
    "@stripe/react-stripe-js": "^4.0.2",
    "@stripe/stripe-js": "^7.9.0",
    "@supabase/supabase-js": "^2.51.0",
    "axios": "^1.10.0",
    "canvas-confetti": "^1.9.3",
    "classnames": "^2.5.1",
    "clsx": "^2.1.1",
    "connect-history-api-fallback": "^2.0.0",
    "cors": "^2.8.5",
    "csv-parser": "^3.2.0",
    "dotenv": "^17.2.0",
    "express": "^5.1.0",
    "framer-motion": "^12.23.18",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.3",
    "lucide-react": "^0.525.0",
    "multer": "^2.0.1",
    "pg": "^8.16.3",
    "qrcode.react": "^4.2.0",
    "react": "^19.1.0",
    "react-barcode": "^1.6.1",
    "react-dom": "^19.1.0",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.6.3",
    "react-toastify": "^11.0.5",
    "recharts": "^3.1.0",
    "stripe": "^18.5.0",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@types/canvas-confetti": "^1.9.0",
    "@types/classnames": "^2.3.0",
    "@types/connect-history-api-fallback": "^1.5.4",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.0.14",
    "@types/pg": "^8.15.4",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react": "^4.6.0",
    "autoprefixer": "^10.4.21",
    "concurrently": "^8.2.2",
    "eslint": "^9.30.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "nodemon": "^3.1.10",
    "postcss": "^8.5.6",
    "tailwind-scrollbar": "^3.0.5",
    "tailwindcss": "^3.4.3",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.3",
